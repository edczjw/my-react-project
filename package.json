{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "alias": "^0.0.5",
    "antd": "^4.16.2",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "sass": "^1.34.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.29.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts,js,jsx}": [
      "prettier --write",
      "eslint --fix --ignore-pattern **/*.d.ts"
    ],
    "src/**/*.{scss,css}": [
      "stylelint --fix"
    ],
    "src/**/*.{json,md}": [
      "prettier --write"
    ]
  }
}
